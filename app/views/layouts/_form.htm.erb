<%= form_with, url: (log_in_path | sign_up_path) do |f| %>
  <p>
    <%= f.label 'email:'%><br>
    <%= f.text_field :email, id: 'email' %>
  </p>
  <p>
    <%= f.label 'password:'%><br>
    <%= f.password_field :password, id: 'password' %>
  </p>
  <p>
    <%= f.submit log_in_path | sign_up_path %>
  </p>
<% end %>